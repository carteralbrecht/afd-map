{
  "name": "afdmap",
  "version": "1.0.0",
  "description": "Interactive AFD Map",
  "scripts": {
    "start": "npm run parcel-setup && parcel",
    "parcel-setup": "cp -R ./node_modules/cesium/Build/Cesium/Assets ./dist && cp -R ./node_modules/cesium/Build/Cesium/ThirdParty ./dist && cp -R ./node_modules/cesium/Build/Cesium/Widgets ./dist && cp -R ./node_modules/cesium/Build/Cesium/Workers ./dist",
    "build": "npm run parcel-setup && parcel build"
  },
  "source": "src/index.html",
  "staticFiles": {
    "staticPath": "node_modules/cesium/Build/Cesium/[Assets/**][ThirdParty/**][Widgets/**][Workers/**]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carteralbrecht/afd-map.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carteralbrecht/afd-map/issues"
  },
  "homepage": "https://github.com/carteralbrecht/afd-map#readme",
  "dependencies": {
    "cesium": "^1.96.0"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.4"
  }
}
